<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="700.0" prefWidth="1400.0" style="-fx-background-color: #001423; -fx-background-radius: 20;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.QuizHomeController">
   <children>
      <Label layoutX="180.0" layoutY="153.0" prefHeight="95.0" prefWidth="229.0" text="Quiz" textFill="#f1faee">
         <font>
            <Font name="Helvetica Neue" size="80.0" />
         </font>
      </Label>
      <Button fx:id="addQuiz" layoutX="180.0" layoutY="262.0" mnemonicParsing="false" onAction="#switchToAddQuiz" prefHeight="43.0" prefWidth="250.0" style="-fx-background-color: #F1FAEE; -fx-background-radius: 20;" text="Add Quiz" textFill="#001423">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Button layoutX="454.0" layoutY="262.0" mnemonicParsing="false" prefHeight="43.0" prefWidth="250.0" style="-fx-background-color: #F1FAEE; -fx-background-radius: 20;" text="Show All" onAction="#switchToDeleteQuiz" textFill="#001423">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Label layoutX="180.0" layoutY="364.0" prefHeight="95.0" prefWidth="413.0" text="Question" textFill="#f1faee">
         <font>
            <Font name="Helvetica Neue" size="80.0" />
         </font>
      </Label>
      <Button fx:id="addQuestion" layoutX="179.0" layoutY="474.0" mnemonicParsing="false" onAction="#switchToAddQuestion" prefHeight="43.0" prefWidth="250.0" style="-fx-background-color: #F1FAEE; -fx-background-radius: 20;" text="Add Question" textFill="#001423">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Button fx:id="deleteId" layoutX="451.0" layoutY="474.0" mnemonicParsing="false" onAction="#switchToDeleteQuestion" prefHeight="43.0" prefWidth="250.0" style="-fx-background-color: #F1FAEE; -fx-background-radius: 20;" text="Show All" textFill="#001423">
         <font>
            <Font size="28.0" />
         </font></Button>
      <Button fx:id="playQuizHome" layoutX="1130.0" layoutY="618.0" mnemonicParsing="false" onAction="#switchToPlayQuiz" prefHeight="37.0" prefWidth="248.0" style="-fx-background-color: #f1faee; -fx-background-radius: 20;" text="Play">
         <font>
            <Font size="30.0" />
         </font></Button>
      <Label layoutX="182.0" layoutY="128.0" text="You Want To Edit?" textFill="#f1faee">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
      <Label layoutX="1131.0" layoutY="571.0" text="Or You Want To Play Instead?" textFill="#f1faee">
         <font>
            <Font size="19.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
